@using Microsoft.AspNet.Identity
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title></title>
    <!--<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">-->
    <meta name="description" content="">
    <meta name="author" content="">

    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

    <link rel="stylesheet" type="text/css" media="screen" href="~/Areas/Admin/Content/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" media="screen" href="~/Areas/Admin/Content/css/font-awesome.min.css">

    <!-- SmartAdmin Styles : Please note (smartadmin-production.css) was created using LESS variables -->
    <link rel="stylesheet" type="text/css" media="screen" href="~/Areas/Admin/Content/css/smartadmin-production.min.css">
    <link rel="stylesheet" type="text/css" media="screen" href="~/Areas/Admin/Content/css/smartadmin-skins.min.css">

    <!-- FAVICONS -->
    <link rel="shortcut icon" href="~/Areas/Admin/Content/img/favicon/favicon.ico" type="image/x-icon">
    <link rel="icon" href="~/Areas/Admin/Content/img/favicon/favicon.ico" type="image/x-icon">

    <!-- GOOGLE FONT -->
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,700italic,300,400,700">

    <!-- Specifying a Webpage Icon for Web Clip
             Ref: https://developer.apple.com/library/ios/documentation/AppleApplications/Reference/SafariWebContent/ConfiguringWebApplications/ConfiguringWebApplications.html -->
    <link rel="apple-touch-icon" href="~/Areas/Admin/Content/img/splash/sptouch-icon-iphone.png">
    <link rel="apple-touch-icon" sizes="76x76" href="~/Areas/Admin/Content/img/splash/touch-icon-ipad.png">
    <link rel="apple-touch-icon" sizes="120x120" href="~/Areas/Admin/Content/img/splash/touch-icon-iphone-retina.png">
    <link rel="apple-touch-icon" sizes="152x152" href="~/Areas/Admin/Content/img/splash/touch-icon-ipad-retina.png">

    <!-- iOS web-app metas : hides Safari UI Components and Changes Status Bar Appearance -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">

    <!-- Startup image for web apps -->
    <link rel="apple-touch-startup-image" href="~/Areas/Admin/Content/img/splash/ipad-landscape.png" media="screen and (min-device-width: 481px) and (max-device-width: 1024px) and (orientation:landscape)">
    <link rel="apple-touch-startup-image" href="~/Areas/Admin/Content/img/splash/ipad-portrait.png" media="screen and (min-device-width: 481px) and (max-device-width: 1024px) and (orientation:portrait)">
    <link rel="apple-touch-startup-image" href="~/Areas/Admin/Content/img/splash/iphone.png" media="screen and (max-device-width: 320px)">
</head>
<body class="">
    <!-- possible classes: minified, fixed-ribbon, fixed-header, fixed-width-->
    <!-- HEADER -->
    <header id="header">
        <div id="logo-group">

            <!-- PLACE YOUR LOGO HERE -->
            <span id="logo"> <img src="~/Areas/Admin/Content/img/logo.png" alt="SmartAdmin"> </span>
            <!-- END LOGO PLACEHOLDER -->
        </div>

        <!-- pulled right: nav area -->
        <div class="pull-right">

            <!-- collapse menu button -->
            <div id="hide-menu" class="btn-header pull-right">
                <span> <a href="javascript:void(0);" data-action="toggleMenu" title="Collapse Menu"><i class="fa fa-reorder"></i></a> </span>
            </div>
            <!-- end collapse menu -->
            <!-- #MOBILE -->
            <!-- Top menu profile link : this shows only when top menu is active -->
            <ul id="mobile-profile-img" class="header-dropdown-list hidden-xs padding-5">
                <li class="">
                    <a href="#" class="dropdown-toggle no-margin userdropdown" data-toggle="dropdown">
                        <img src="~/Areas/Admin/Content/img/avatars/sunny.png" alt="John Doe" class="online" />
                    </a>
                    <ul class="dropdown-menu pull-right">
                        <li>
                            <a href="javascript:void(0);" class="padding-10 padding-top-0 padding-bottom-0"><i class="fa fa-cog"></i> Setting</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="profile.html" class="padding-10 padding-top-0 padding-bottom-0"> <i class="fa fa-user"></i> <u>P</u>rofile</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="javascript:void(0);" class="padding-10 padding-top-0 padding-bottom-0" data-action="toggleShortcut"><i class="fa fa-arrow-down"></i> <u>S</u>hortcut</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="javascript:void(0);" class="padding-10 padding-top-0 padding-bottom-0" data-action="launchFullscreen"><i class="fa fa-arrows-alt"></i> Full <u>S</u>creen</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="login.html" class="padding-10 padding-top-5 padding-bottom-5" data-action="userLogout"><i class="fa fa-sign-out fa-lg"></i> <strong><u>L</u>ogout</strong></a>
                        </li>
                    </ul>
                </li>
            </ul>

            <!-- logout button -->
            <div id="logout" class="btn-header transparent pull-right">
                <span> <a href="@Url.RouteUrl(new { controller="Account", action="Logout"})" title="Sign Out" data-action="userLogout" data-logout-msg="You can improve your security further after logging out by closing this opened browser"><i class="fa fa-sign-out"></i></a> </span>
            </div>
            <!-- end logout button -->
            <!-- fullscreen button -->
            <div id="fullscreen" class="btn-header transparent pull-right">
                <span> <a href="javascript:void(0);" data-action="launchFullscreen" title="Full Screen"><i class="fa fa-arrows-alt"></i></a> </span>
            </div>
            <!-- end fullscreen button -->
        </div>
        <!-- end pulled right: nav area -->
    </header>
    <!-- END HEADER -->
    <!-- Left panel : Navigation area -->
    <!-- Note: This width of the aside area can be adjusted through LESS variables -->
    <aside id="left-panel">

        <!-- User info -->
        <div class="login-info">
            <span>
                <!-- User image size is adjusted inside CSS, it should stay as it -->

                <a href="javascript:void(0);" id="show-shortcut" data-action="toggleShortcut">
                    <img src="~/Areas/Admin/Content/img/avatars/sunny.png" alt="me" class="online" style="display: none" />
                    <span>
                        @User.Identity.GetUserName()
                    </span>
                    <i class="fa fa-angle-down" style="display:none"></i>
                </a>

            </span>
        </div>
        <!-- end user info -->
        <!-- NAVIGATION : This navigation is also responsive

        To make this navigation dynamic please make sure to link the node
        (the reference to the nav > ul) after page load. Or the navigation
        will not initialize.
        -->
        <nav>
            <!-- NOTE: Notice the gaps after each icon usage <i></i>..
            Please note that these links work a bit different than
            traditional href="" links. See documentation for details.
            -->

            <ul>
                <li class="active">
                    <a href="@Url.RouteUrl(new { controller = "Dashboard", action = "Index" })" title="Dashboard"><i class="fa fa-lg fa-fw fa-home"></i> <span class="menu-item-parent">Dashboard</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "Admin", action = "Index" })"><i class="fa fa-lg fa-fw fa-user"></i> <span class="menu-item-parent">Usuarios</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "Tag", action = "List" })"><i class="fa fa-lg fa-fw fa-tags"></i> <span class="menu-item-parent">Tags</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "Pagina", action = "List" })"><i class="fa fa-lg fa-fw fa-copy"></i> <span class="menu-item-parent">Paginas</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "ViajeTipo1", action = "List", TipoViaje = 1, SubTipoViaje = 1 })"><i class="fa fa-lg fa-fw fa-paper-plane-o"></i> <span class="menu-item-parent">Programa</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "ViajeTipo1", action = "List", TipoViaje = 1, SubTipoViaje = 2 })"><i class="fa fa-lg fa-fw fa-life-ring"></i> <span class="menu-item-parent">Crucero</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "ViajeTipo1", action = "List", TipoViaje = 1, SubTipoViaje = 4 })"><i class="fa fa-lg fa-fw fa-life-ring"></i> <span class="menu-item-parent">Wellness & spa</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "ViajeTipo1", action = "List", TipoViaje = 1, SubTipoViaje = 3 })"><i class="fa fa-lg fa-fw fa-truck"></i> <span class="menu-item-parent">Tren</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "ViajeTipo1", action = "List", TipoViaje = 2, SubTipoViaje = 1 })"><i class="fa fa-lg fa-fw fa-building"></i> <span class="menu-item-parent">Hotel</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "ViajeTipo1", action = "List", TipoViaje = 2, SubTipoViaje = 2 })"><i class="fa fa-lg fa-fw fa-flag"></i> <span class="menu-item-parent">Excursion</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "Partner", action = "List" })"><i class="fa fa-lg fa-fw fa-list-alt"></i> <span class="menu-item-parent">Partners</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "Region", action = "List" })"><i class="fa fa-lg fa-fw fa-globe"></i> <span class="menu-item-parent">Regiones</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "Sugerencia", action = "List" })"><i class="fa fa-lg fa-fw fa-ticket"></i> <span class="menu-item-parent">Sugerencias</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "Promocion", action = "List" })"><i class="fa fa-lg fa-fw fa-bullhorn"></i> <span class="menu-item-parent">Promociones</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "Prensa", action = "List" })"><i class="fa fa-lg fa-fw fa-file-pdf-o"></i> <span class="menu-item-parent">Prensa</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "FormularioSesion", action = "Edit" })"><i class="fa fa-lg fa-fw fa-asterisk"></i> <span class="menu-item-parent">Sesiones informativas</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "Nota", action = "List" })"><i class="fa fa-lg fa-fw fa-asterisk"></i> <span class="menu-item-parent">Notas</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "BannerHome", action = "List" })"><i class="fa fa-lg fa-fw fa-asterisk"></i> <span class="menu-item-parent">Banner Home</span></a>
                </li>
                <li>
                    <a href="@Url.RouteUrl(new { controller = "Contacto", action = "Index" })"><i class="fa fa-lg fa-fw fa-asterisk"></i> <span class="menu-item-parent">Emails</span></a>
                </li>
            </ul>
        </nav>
        <span class="minifyme" data-action="minifyMenu">
            <i class="fa fa-arrow-circle-left hit"></i>
        </span>
    </aside>
    <!-- END NAVIGATION -->

    <!-- MAIN PANEL -->
    <div id="main" role="main">
        <!-- RIBBON -->
        <div id="ribbon">

            <!--
            <span class="ribbon-button-alignment">
                <span id="refresh" class="btn btn-ribbon" data-action="resetWidgets" data-title="refresh" rel="tooltip" data-placement="bottom" data-original-title="<i class='text-warning fa fa-warning'></i> Warning! This will reset all your widget settings." data-html="true">
					<i class="fa fa-refresh"></i>
				</span>
            </span>
                -->

            <!-- breadcrumb 
            <ol class="breadcrumb">
                <li>Home</li>
                <li>Dashboard</li>
            </ol>-->
            <!-- end breadcrumb -->

        </div>
        <!-- END RIBBON -->
        <!-- MAIN CONTENT -->
        <div id="content">
            @RenderBody()
        </div>
        <!-- END MAIN CONTENT -->

    </div>
    <!-- END MAIN PANEL -->
    <!-- PAGE FOOTER -->
    <div class="page-footer">
        <div class="row">
            <div class="col-xs-12 col-sm-6">
                <span class="txt-color-white">3Pi Factory</span>
            </div>
        </div>
    </div>
    <!-- END PAGE FOOTER -->

    <script src="~/Areas/Admin/Content/js/mine.js"></script>

    <!-- Link to Google CDN's jQuery + jQueryUI; fall back to local -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
    <script>
        if (!window.jQuery) {
            document.write('<script src="~/Areas/Admin/Content/js/libs/jquery-2.0.2.min.js"><\/script>');
        }
    </script>

    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
    <script>
        if (!window.jQuery.ui) {
            document.write('<script src="~/Areas/Admin/Content/js/libs/jquery-ui-1.10.3.min.js"><\/script>');
        }
    </script>

    <!-- IMPORTANT: APP CONFIG -->
    <script src="~/Areas/Admin/Content/js/app.config.js"></script>

    <!-- JS TOUCH : include this plugin for mobile drag / drop touch events-->
    <script src="~/Areas/Admin/Content/js/plugin/jquery-touch/jquery.ui.touch-punch.min.js"></script>

    <!-- BOOTSTRAP JS -->
    <script src="~/Areas/Admin/Content/js/bootstrap/bootstrap.min.js"></script>

    <!-- CUSTOM NOTIFICATION -->
    <script src="~/Areas/Admin/Content/js/notification/SmartNotification.min.js"></script>

    <!-- JARVIS WIDGETS -->
    <script src="~/Areas/Admin/Content/js/smartwidgets/jarvis.widget.min.js"></script>

    <!-- EASY PIE CHARTS -->
    <script src="~/Areas/Admin/Content/js/plugin/easy-pie-chart/jquery.easy-pie-chart.min.js"></script>

    <!-- SPARKLINES -->
    <script src="~/Areas/Admin/Content/js/plugin/sparkline/jquery.sparkline.min.js"></script>

    <!-- JQUERY VALIDATE -->
    <script src="~/Areas/Admin/Content/js/plugin/jquery-validate/jquery.validate.min.js"></script>

    <!-- JQUERY MASKED INPUT -->
    <script src="~/Areas/Admin/Content/js/plugin/masked-input/jquery.maskedinput.min.js"></script>

    <!-- JQUERY SELECT2 INPUT -->
    <script src="~/Areas/Admin/Content/js/plugin/select2/select2.min.js"></script>

    <!-- JQUERY UI + Bootstrap Slider -->
    <script src="~/Areas/Admin/Content/js/plugin/bootstrap-slider/bootstrap-slider.min.js"></script>

    <!-- browser msie issue fix -->
    <script src="~/Areas/Admin/Content/js/plugin/msie-fix/jquery.mb.browser.min.js"></script>

    <!-- FastClick: For mobile devices -->
    <script src="~/Areas/Admin/Content/js/plugin/fastclick/fastclick.min.js"></script>

    <!--[if IE 8]>

    <h1>Your browser is out of date, please update your browser by going to www.microsoft.com/download</h1>

    <![endif]-->

    <!-- MAIN APP JS FILE -->
    <script src="~/Areas/Admin/Content/js/app.min.js"></script>

    <script>
    $(document).ready(function() {

        // DO NOT REMOVE : GLOBAL FUNCTIONS!
        pageSetUp();
    });
    </script>

    @RenderSection("scripts", required: false)
</body>
</html>